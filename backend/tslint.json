{
  "extends": ["tslint:recommended", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": ["node_modules/**/*.ts"]
  },
  "rules": {
    "interface-name": [true, "never-prefix"],
    "object-literal-sort-keys": false,
    "no-console": false
  },
  "jsRules": {
    "object-literal-sort-keys": false,
    "no-console": false
  }
}
